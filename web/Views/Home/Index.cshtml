    <h3><br />Exchange Rates</h3>
<h4><br />Daily Exchange Rate Update Process runs at 9:00am</h4>
<div ng-controller="mainController" ng-cloak>
    <md-button class="md-fab" aria-label="Eat cake">
        <md-icon md-svg-src="img/icons/cake.svg"></md-icon>
    </md-button>
    <table class="table table-striped">
        <tr>
            <th>Bank Name</th>
            <th>Australian Dollar Buying Rate</th>
            <th>Australian Dollar Selling Rate</th>
            <th>Euro Buying Rate</th>
            <th>Euro Selling Rate</th>
            <th>Norwegian Krone Buying Rate</th>
            <th>Norwegian Krone Selling Rate</th>
            <th>American Dollar Buying Rate</th>
            <th>American Dollar Selling Rate</th>
            <th>Description</th>
        </tr>

        <tr ng-repeat="rate in rates">

            <td>{{rate.bankName}}</td>
            <td>{{rate.ADBR}}</td>
            <td>{{rate.ADSR}}</td>
            <td>{{rate.EUBR}}</td>
            <td>{{rate.EUSR}}</td>
            <td>{{rate.NORBR}}</td>
            <td>{{rate.NORSR}}</td>
            <td>{{rate.USBR}}</td>
            <td>{{rate.USSR}}</td>
            <td>{{rate.SiteUpdated}}</td>

            @*<td class="text-right">
                    <button class="btn btn-danger" ng-click="deleteProduct(product)">X</button>
                </td>*@
        </tr>

    </table>
    
